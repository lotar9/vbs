<div class="panel panel-default">
	<div class="panel-heading">Resultados {{header}}</div>
	<div class="panel-body">
		{{#matches}}
			<div class="row col-sm-12">
				{{category_name}}
				<div class="row">
				{{#match}}
					<div class="col-sm-3">
						{{category}} {{_date}}<br/>
						{{local}}  - {{visiting}}
						<br/><small>{{location}}</small>
					</div>
				{{/match}}
				</div>
			</div>
		{{/matches}}
	</div>
</div>
